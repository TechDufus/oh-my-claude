{
  "command": "rust-analyzer",
  "args": [],
  "transport": "stdio",
  "extensionToLanguage": {
    ".rs": "rust"
  },
  "initializationOptions": {
    "cargo": {
      "allFeatures": true
    },
    "checkOnSave": {
      "command": "clippy"
    }
  },
  "settings": {
    "rust-analyzer": {
      "diagnostics": {
        "enable": true
      },
      "inlayHints": {
        "enable": true
      }
    }
  }
}
